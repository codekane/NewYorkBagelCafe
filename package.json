{
  "name": "flightdeck",
  "version": "0.9.0",
  "description": "Flight Deck is an opinionated starter project for Jekyll that uses modern front-end tooling.",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "bundle install --path vendor/bundle",
    "start": "bundle exec gulp",
    "test": "bundle exec gulp test",
    "browsersync": "bundle exec gulp browsersync",
    "browsersync-production": "bundle exec gulp browsersync --jekyllEnv='production'",
    "build": "bundle exec gulp build --jekyllEnv='production'",
    "deploy": "bundle exec gulp deploy",
    "eslint": "bundle exec gulp eslint",
    "imagemin": "bundle exec gulp imagemin",
    "production": "bundle exec gulp --jekyllEnv='production'",
    "sass": "bundle exec gulp sass",
    "watch": "bundle exec gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ginfuru/Flightdeck.git"
  },
  "keywords": [
    "Autoprefixer",
    "Browsersync",
    "gulp",
    "imagemin",
    "Jekyll",
    "PostCSS",
    "Sass",
    "Webpack"
  ],
  "author": "Ginfuru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ginfuru/Flightdeck/issues"
  },
  "homepage": "https://github.com/ginfuru/Flightdeck/",
  "devDependencies": {
    "autoprefixer": "^9.4.8",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.6",
    "browserslist": "^4.6.2",
    "cryptiles": ">=4.1.2",
    "eslint-config-google": "^0.9.1",
    "eslint-loader": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rsync": "^0.0.8",
    "gulp-sass": "^4.0.1",
    "gulp-watch": "^5.0.0",
    "imagemin-pngquant": "^6.0.0",
    "natives": "^1.1.6",
    "require-dir": "^1.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.29.5",
    "webpack-stream": "^5.0.0",
    "yargs": "^12.0.1"
  },
  "dependencies": {
    "spectre.css": "^0.5.8"
  }
}
